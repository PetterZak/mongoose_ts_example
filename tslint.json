{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": {
      "options": "single"
    },
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "max-line-length": [
      true,
      160
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-console": false,
    "typedef": [true, "call-signature"],
    "variable-name": [true, "allow-leading-underscore"]
  },
  "rulesDirectory": []
}